{
  "version": "0.2.1",
  "id": "@shopgate/grouped-products",
  "components": [
    {
      "id": "GroupedProducts",
      "path": "frontend/portals/GroupedProducts/index.jsx",
      "target": "product.variant-select.before",
      "type": "portals"
    },
    {
      "id": "StyleOverwrites",
      "path": "frontend/portals/StyleOverwrites/index.jsx",
      "target": "app.globals",
      "type": "portals"
    },
    {
      "id": "CTAButtons",
      "path": "frontend/portals/CTAButtons/index.jsx",
      "target": "product.ctas",
      "type": "portals"
    },
    {
      "id": "ProductHeader",
      "path": "frontend/portals/ProductHeader/index.jsx",
      "target": "product.header.before",
      "type": "portals"
    },
    {
      "id": "Subscribers",
      "path": "frontend/subscriptions/index.js",
      "type": "subscribers"
    },
    {
      "type": "translations",
      "path": "frontend/locale/en-US.json",
      "id": "locale/en-US"
    },
    {
      "type": "translations",
      "path": "frontend/locale/de-DE.json",
      "id": "locale/de-DE"
    }
  ],
  "configuration": {
    "maxQuantityPickerEntries": {
      "type": "static",
      "destination": "frontend",
      "params": {
        "value": 50
      }
    }
  }
}
